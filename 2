#include <iostream>

long long nod(long long a, long long b) {
    while (b != 0) {
        long long temp = b;
        b = a % b;
        a = temp;
    }
    return a;
}

long long nok(long long a, long long b) {
    return (a / nod(a, b)) * b;
}

int main() {
    long long a, b;

    std::cin >> a >> b;
    std::cout << nok(a, b);
}
